local simradius = 'shp'
--METHOD THING
if shp and (simradius == "shp") then
	AlternativeSpawn(function()
		while Character and game:GetService("RunService").Heartbeat:Wait() do
			shp(User, "SimulationRadius", math.huge)
		end
	end)
elseif ssr and (simradius == "ssr") then
	AlternativeSpawn(function()
		while Character and game:GetService("RunService").Heartbeat:Wait() do
			ssr(math.huge)
		end
	end)
end
